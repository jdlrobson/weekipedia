{
  "extends": "wikimedia",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "Request": false,
    "require": false,
    "module": false,
    "fetch": false,
    "Promise": false,
    "window": false,
    "history": false
  },
  "rules": {
    "react/no-unused-state": 1,
    "react/no-unused-prop-types": 1,
    "camelcase": 0,
    "no-useless-escape": 0,
    "one-var": 0,
    "vars-on-top": 0
  }
}